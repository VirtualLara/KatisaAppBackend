{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "totalpago": {
      "type": "decimal",
      "required": true
    },
    "users": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "direccionenvio": {
      "type": "json",
      "required": true
    },
    "idpago": {
      "type": "string",
      "required": true
    },
    "product": {
      "model": "product"
    },
    "productprecio": {
      "type": "decimal",
      "required": true
    },
    "cantidad": {
      "type": "integer",
      "required": true
    }
  }
}
